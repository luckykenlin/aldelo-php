{
  "name": "luckykenlin/aldelo-php",
  "description": "Aldelo APIs PHP Helper Library SDK.",
  "keywords": [
    "Aldelo"
  ],
  "license": "MIT",
  "homepage": "https://github.com/luckykenlin/aldelo-php",
  "support": {
    "issues": "https://github.com/luckykenlin/aldelo-php/issues",
    "source": "https://github.com/luckykenlin/aldelo-php"
  },
  "authors": [
    {
      "name": "Ken lin",
      "email": "luckykenlin@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": ">=5.7.0",
    "laravel/helpers": "^1.1",
    "phpseclib/phpseclib": "^2.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0|~5.0",
    "fzaninotto/faker": "~1.4"
  },
  "autoload": {
    "psr-4": {
      "Luckykenlin\\Aldelo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Luckykenlin\\Aldelo\\Providers\\ServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}